package itemDto;

public class UserDto {

		String name;
		int pass;
		int id;
		
		public String getName() {
			return name;
		}
		
		public int getPass() {
			return pass;
		}
		
		public int getId() {
			return id;
		}
		
		public void setName(String name) {
			this.name = name;
		}
		
		public void setPass(int pass) {
			this.pass = pass;
		}
		
		public void setId(int id) {
			this.id = id;
		}
	
}
